{
	"presets": [
		"@babel/preset-react",
		"@babel/preset-flow",
		[
			"@babel/preset-env",
			{
				"targets": {
					"node": "8.10.0"
				}
			}
		]
	],
	"env": {
		"development": {
			"plugins": ["flow-react-proptypes"]
		}
	},
	"plugins": [
		"babel-plugin-styled-components",
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-proposal-object-rest-spread",
		"react-hot-loader/babel",
		"ramda"
	]
}
