{
	"name": "mappslab-beta",
	"version": "0.0.1",
	"description": "",
	"main": "index.js",
	"scripts": {
		"test": "jest",
		"dev:app": "cross-env ENV=development DEBUG=api webpack-dev-server --colors --env development --config ./config/webpack.config.js",
		"dev:api": "cross-env ENV=development cross-env DEBUG=api nodemon --watch api api/server.js --exec babel-node",
		"db:stopall": "docker stop $(docker ps -q)",
		"db:test:init": "docker-compose -p mappslab-test -f ./api/__tests__/docker-compose.yml up -d",
		"db:test:start": "docker start mappslab-test_dgraph-zero mappslab-test_dgraph-server mappslab-test_dgraph-ratel",
		"db:test:seed": "cross-env TEST_DB=true cross-env DEBUG=api:testDatabase babel-node ./api/__tests__/scripts/testDatabase/seedTestDatabase.js",
		"db:test:stop": "docker stop mappslab-test_dgraph-zero mappslab-test_dgraph-server mappslab-test_dgraph-ratel",
		"db:staging:seed": "cross-env ENV=staging npm run dev:db:seed",
		"build:app": "webpack -p --env production --config ./config/webpack.config.js --progress --colors",
		"build:api": "cross-env ENV=production babel ./api -d ./build/api",
		"flow:install-types": "flow-typed install",
		"test:app": "cross-env ENV=development cross-env DEBUG=test cross-env TEST_DB=true jest --projects ./app --coverage",
		"test:app:watch": "cross-env ENV=development cross-env DEBUG=test cross-env TEST_DB=true jest --projects ./app --watch",
		"test:api": "cross-env ENV=development cross-env DEBUG=test cross-env TEST_DB=true jest --projects ./api --coverage",
		"test:api:watch": "cross-env ENV=development cross-env DEBUG=test cross-env TEST_DB=true jest --projects ./api --watch"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/good-idea/mappslab-beta.git"
	},
	"keywords": [
		"mapping",
		"education"
	],
	"author": "Joseph Thomas",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/good-idea/mappslab-beta/issues"
	},
	"homepage": "https://github.com/good-idea/mappslab-beta#readme",
	"workspaces": [
		"api",
		"app",
		"packages/*"
	],
	"private": true,
	"devDependencies": {
		"@babel/cli": "7.0.0-beta.44",
		"@babel/core": "7.0.0-beta.44",
		"@babel/node": "7.0.0-beta.44",
		"@babel/plugin-proposal-class-properties": "^7.0.0-beta.47",
		"@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.44",
		"@babel/plugin-transform-runtime": "7.0.0-beta.44",
		"@babel/polyfill": "7.0.0-beta.39",
		"@babel/preset-env": "7.0.0-beta.44",
		"@babel/preset-flow": "7.0.0-beta.44",
		"@babel/preset-react": "7.0.0-beta.39",
		"@babel/preset-stage-3": "^7.0.0-beta.41",
		"@babel/register": "7.0.0-beta.44",
		"@babel/runtime": "7.0.0-beta.44",
		"babel-core": "7.0.0-bridge.0",
		"babel-eslint": "^8.2.3",
		"babel-jest": "^22.4.3",
		"babel-loader": "^8.0.0-beta.0",
		"babel-plugin-flow-react-proptypes": "^24.0.0",
		"babel-plugin-import": "^1.7.0",
		"babel-plugin-module-resolver": "^3.1.1",
		"babel-plugin-ramda": "^1.6.1",
		"babel-plugin-styled-components": "^1.5.1",
		"babel-plugin-transform-imports": "^1.5.0",
		"babel-watch": "git+https://git@github.com/good-idea/babel-watch.git#babel7",
		"backpack-core": "^0.8.0-0",
		"cross-env": "^5.1.4",
		"css-loader": "^0.28.11",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"enzyme-to-json": "^3.3.4",
		"eslint": "^4.19.1",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-config-prettier": "^2.9.0",
		"eslint-import-resolver-webpack": "^0.9.0",
		"eslint-plugin-flowtype": "^2.46.3",
		"eslint-plugin-import": "^2.11.0",
		"eslint-plugin-jsx-a11y": "^6.0.3",
		"eslint-plugin-react": "^7.7.0",
		"flow-bin": "^0.71.0",
		"flow-typed": "^2.4.0",
		"jest": "^22.4.3",
		"jest-styled-components": "^5.0.1",
		"lodash-webpack-plugin": "^0.11.5",
		"npm-run-all": "^4.1.2",
		"prettier": "1.12.1",
		"react-hot-loader": "^4.1.2",
		"react-test-renderer": "^16.3.2",
		"rimraf": "^2.6.2",
		"sinon": "^5.0.4",
		"style-loader": "^0.21.0",
		"webpack": "^4.7.0",
		"webpack-cli": "^2.1.3",
		"webpack-dev-server": "^3.1.4",
		"webpack-merge": "^4.1.2"
	}
}
