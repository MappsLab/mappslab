{
	"presets": ["@babel/preset-flow", "@babel/preset-env"],
	"plugins": [
		[
			"babel-plugin-module-resolver",
			{
				"root": ["./src"],
				"alias": {
					"App": "./app",
					"Api": "./api",
					"Styles": "./app/styles",
					"mapp": "./packages/mapp/src"
				},
				"cwd": "babelrc"
			}
		],
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-proposal-object-rest-spread",
	]
}
