{
	"name": "mappslab-api",
	"version": "0.0.1",
	"description": "Express-based central API backend for MappsLab",
	"main": "index.js",
	"author": "Joseph Thomas | Good Idea Studio",
	"license": "ISC",
	"scripts": {
		"start": "node ./server.js",
		"build:api": "cross-env ENV=production babel . -d ../build/api",
		"dev": "cross-env ENV=development cross-env DEBUG=api nodemon --watch . server.js --exec babel-node",
		"test": "cross-env ENV=development cross-env DEBUG=test cross-env TEST_DB=true jest --coverage",
		"test:watch": "cross-env ENV=development cross-env DEBUG=test cross-env TEST_DB=true jest --watch",
		"db:test:seed": "cross-env TEST_DB=true cross-env DEBUG=seed babel-node ./__tests__/scripts/testDatabase/seedTestDatabase.js",
		"db:staging:seed": "cross-env ENV=staging cross-env DEBUG=seed babel-node ./__tests__/scripts/testDatabase/seedTestDatabase.js"
	},
	"dependencies": {
		"bcrypt": "^3.0.0",
		"crypto-promise": "^2.0.0",
		"debug": "^3.1.0",
		"dgraph-js": "^1.2.1",
		"dotenv": "^5.0.1",
		"faker": "^4.1.0",
		"flat": "^4.1.0",
		"graphql": "^0.13.2",
		"graphql-subscriptions": "^0.5.8",
		"graphql-tools": "^3.0.5",
		"graphql-yoga": "^1.14.12",
		"grpc": "^1.13.0",
		"joi": "^13.4.0",
		"json5": "^1.0.1",
		"jsonwebtoken": "^8.2.1",
		"lodash": "^4.17.10",
		"prop-types": "^15.6.1",
		"ramda": "^0.25.0",
		"validator": "^10.4.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.0.0",
		"@babel/core": "^7.0.0",
		"@babel/node": "^7.0.0",
		"@babel/plugin-proposal-class-properties": "^7.0.0-beta.53",
		"@babel/plugin-proposal-export-default-from": "^7.0.0-beta.54",
		"@babel/plugin-proposal-object-rest-spread": "^7.0.0",
		"@babel/plugin-transform-runtime": "^7.0.0",
		"@babel/polyfill": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"@babel/preset-flow": "^7.0.0",
		"@babel/register": "^7.0.0",
		"@babel/runtime": "^7.0.0",
		"babel-core": "7.0.0-bridge.0",
		"babel-eslint": "^8.2.6",
		"babel-jest": "^23.4.0",
		"babel-plugin-import": "^1.8.0",
		"babel-plugin-module-resolver": "^3.1.1",
		"cross-env": "^5.2.0",
		"eslint": "^5.1.0",
		"eslint-config-airbnb": "^17.0.0",
		"eslint-config-prettier": "^2.9.0",
		"eslint-import-resolver-webpack": "^0.10.1",
		"eslint-plugin-flowtype": "^2.50.0",
		"eslint-plugin-import": "^2.13.0",
		"eslint-plugin-jsx-a11y": "^6.1.1",
		"eslint-plugin-react": "^7.10.0",
		"flow-bin": "^0.76.0",
		"flow-typed": "^2.5.1",
		"nodemon": "^1.18.2",
		"rimraf": "^2.6.2"
	}
}
