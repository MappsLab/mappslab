  "scripts": {
    "start": "serve . --single",
    "build": "webpack -p --env production --progress --colors --config webpack.config.js",
    "dev": "cross-env ENV=development DEBUG=api webpack-dev-server --colors --env development --config webpack.config.js",
    "dev:staging": "cross-env ENV=development cross-env AWS_BUCKET_ROOT=https://mappslab-beta.s3.amazonaws.com DEBUG=api webpack-dev-server --colors --env development --config webpack.config.js",
    "install-types": "typesync && yarn install",
    "generate-types": "graphql get-schema && graphql-codegen && prettier ./src/types-ts/**/* --write",
    "test": "cross-env ENV=development cross-env DEBUG=test cross-env TEST_DB=true jest ",
    "test:coverage": "cross-env ENV=development cross-env DEBUG=test cross-env TEST_DB=true jest ",
    "test:watch": "cross-env ENV=development cross-env DEBUG=test cross-env TEST_DB=true jest --watch",
    "deploy": "yarn build && cd public && now -t $ZEIT_TOKEN && now alias -t $ZEIT_TOKEN",
    "deploy:dev": "yarn build && cd public && now && now alias",
    "update-mock-schema": "graphql get-schema mappslab"
  },
